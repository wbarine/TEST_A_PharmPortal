def add_numbers(a, b):
    return a + b

def subtract_numbers(a, b):
    return a - b

def multiply_numbers(a, b):
    return a * b

def divide_numbers(a, b):
    if b == 0:
        raise ValueError("Cannot divide by zero")
    return a / b

def square_number(a):
    return a ** 2

def cube_number(a):
    return a ** 3

# Test the functions
print(add_numbers(2, 3))
print(subtract_numbers(5, 2))
print(multiply_numbers(4, 6))
print(divide_numbers(10, 2))
print(square_number(4))
print(cube_number(3))
